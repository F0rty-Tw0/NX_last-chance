{
  "projectType": "library",
  "sourceRoot": "libs/frontend/web-app/storybook/src",
  "prefix": "last-chance",
  "targets": {
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/frontend/web-app/storybook/.storybook",
        "browserTarget": "frontend-web-app-storybook:build-storybook",
        "compodoc": false,
        "styles": ["libs/frontend/shared/styles/src/index.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["libs/frontend/shared/styles/src"]
        }
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputDir": "dist/storybook/frontend-web-app-storybook",
        "configDir": "libs/frontend/web-app/storybook/.storybook",
        "browserTarget": "frontend-web-app-storybook:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "implicitDependencies": ["frontend-shared-assets", "frontend-shared-styles"],
  "tags": ["layer:frontend", "scope:web-app", "type:storybook"]
}
